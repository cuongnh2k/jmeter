import groovy.json.JsonGenerator 
import org.skyscreamer.jsonassert.JSONAssert
import org.skyscreamer.jsonassert.JSONCompareMode

// JSON Generator with UTF-8
def jsonGenerator = new JsonGenerator.Options()
					.dateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ssZZ&quot;)
					.timezone(&quot;Asia/Ho_Chi_Minh&quot;)
					.disableUnicodeEscaping()
					.build()
log.debug(&quot;Connected to database:&quot;)
// Instance SQL
String dbUrl = vars.get(&apos;DB_URL&apos;)
String dbUsername = vars.get(&apos;DB_USERNAME&apos;)
String dbPassword = vars.get(&apos;DB_PASSWORD&apos;)
int id = Integer.valueOf(vars.get(&apos;JOB_ID&apos;))
def sql = Sql.newInstance(dbUrl, dbUsername, dbPassword, &quot;com.mysql.jdbc.Driver&quot;)

log.debug(&quot;Connected to database: ${dbUrl}&quot;)

// Select
def fetch = sql.rows(&quot;&quot;&quot; (SELECT wjp.ID               id,
                oi.NAME              companyName,
                wjt.NAME             type,
                wjp.TITLE            title,
                wjp.SLUG             slug,
                wjp.META_TITLE       metaTitle,
                wjp.META_KEYWORDS    metaKeywords,
                wjp.META_DESCRIPTION metaDescription,
                wjp.NUMBER_REQUEST   numberRequest,
                wjp.END_DATE         endDate,
                wjp.WORK_ADDRESS     workAddress,
                wjp.DESCRIPTION      description,
                wjp.REQUIREMENT      requirement,
                wjp.BENEFIT          benefit,
                wjp.PUBLISHED_AT     publishedAt
         FROM WCP_JOB_POSTS wjp
            INNER JOIN WCP_JOB_TYPE wjt ON wjp.JOB_TYPE_ID = wjt.ID
            INNER JOIN ORG_INFO oi ON wjp.ORG_ID = oi.ID
         WHERE  wjp.IS_DISPLAY = &apos;YES&apos;
            AND wjp.IS_DELETED = &apos;NO&apos;
            AND wjt.IS_DISPLAY = &apos;YES&apos;
            AND wjt.IS_DELETED = &apos;NO&apos;
            AND oi.IS_ACTIVE = &apos;YES&apos;
            AND oi.IS_DELETED = &apos;NO&apos;
            AND wjp.ID = ${id}
        )
        UNION
        (SELECT wjp.ID               id,
                oi.NAME              companyName,
                wjt.NAME             type,
                wjp.TITLE            title,
                wjp.SLUG             slug,
                wjp.META_TITLE       metaTitle,
                wjp.META_KEYWORDS    metaKeywords,
                wjp.META_DESCRIPTION metaDescription,
                wjp.NUMBER_REQUEST   numberRequest,
                wjp.END_DATE         endDate,
                wjp.WORK_ADDRESS     workAddress,
                wjp.DESCRIPTION      description,
                wjp.REQUIREMENT      requirement,
                wjp.BENEFIT          benefit,
                wjp.PUBLISHED_AT     publishedAt
         FROM WCP_JOB_POSTS wjp
            INNER JOIN WCP_JOB_TYPE wjt ON wjp.JOB_TYPE_ID = wjt.ID
            INNER JOIN ORG_INFO oi ON wjp.ORG_ID = oi.ID
         WHERE  wjp.IS_DISPLAY = &apos;YES&apos;
            AND wjp.IS_DELETED = &apos;NO&apos;
            AND wjt.IS_DISPLAY = &apos;YES&apos;
            AND wjt.IS_DELETED = &apos;NO&apos;
            AND oi.IS_ACTIVE = &apos;YES&apos;
            AND oi.IS_DELETED = &apos;NO&apos;
            AND wjp.ID != ${id}
         ORDER BY wjp.PUBLISHED_AT DESC
         LIMIT 5) &quot;&quot;&quot;)
def listData = []
def b
int i=0
fetch.each{ r -&gt; 
  	if(i==0){
		b = [
  		&apos;id&apos;: r.id,
          &apos;companyName&apos;: r.companyName,
          &apos;type&apos;: r.type,
          &apos;title&apos;: r.title,
          &apos;slug&apos;: r.slug,
          &apos;metaTitle&apos;: r.metaTitle,
          &apos;metaKeywords&apos;: r.metaKeywords,
          &apos;metaDescription&apos;: r.metaDescription,
          &apos;numberRequest&apos;: r.numberRequest,
          &apos;workAddress&apos;: r.workAddress,
          &apos;description&apos;: r.description,
          &apos;requirement&apos;: r.requirement,
          &apos;benefit&apos;: r.benefit,
          &apos;publishedAt&apos;: r.publishedAt
          ]
  		
  	}
  	if(i != 0){
  	def a = [
  		&apos;id&apos;: r.id,
          &apos;companyName&apos;: r.companyName,
          &apos;type&apos;: r.type,
          &apos;title&apos;: r.title,
          &apos;slug&apos;: r.slug,
          &apos;metaTitle&apos;: r.metaTitle,
          &apos;metaKeywords&apos;: r.metaKeywords,
          &apos;metaDescription&apos;: r.metaDescription,
          &apos;numberRequest&apos;: r.numberRequest,
          &apos;workAddress&apos;: r.workAddress,
          &apos;description&apos;: r.description,
          &apos;requirement&apos;: r.requirement,
          &apos;benefit&apos;: r.benefit,
          &apos;publishedAt&apos;: r.publishedAt
  	]
  	listData.add(a)}
  	i++
}
        

def data = [
		&apos;id&apos;: b.id,
          &apos;companyName&apos;: b.companyName,
          &apos;type&apos;: b.type,
          &apos;title&apos;: b.title,
          &apos;slug&apos;: b.slug,
          &apos;metaTitle&apos;: b.metaTitle,
          &apos;metaKeywords&apos;: b.metaKeywords,
          &apos;metaDescription&apos;: b.metaDescription,
          &apos;numberRequest&apos;: b.numberRequest,
          &apos;workAddress&apos;: b.workAddress,
          &apos;description&apos;: b.description,
          &apos;requirement&apos;: b.requirement,
          &apos;benefit&apos;: b.benefit,
          &apos;publishedAt&apos;: b.publishedAt,
          &apos;jobNews&apos;: listData
]

// Make expected - from database
String expected = jsonGenerator.toJson(data)
log.debug(expected)

// Get actual - reponse data
prev.setDataEncoding(&quot;UTF-8&quot;)
String actual = prev.getResponseDataAsString()
log.debug(actual)

// Compare expected with actual
JSONAssert.assertEquals(expected, actual, JSONCompareMode.LENIENT)</stringProp>
